<div class="container">
  <h1>Create a new cocktail</h1>
  <%= simple_form_for [@cocktail] do |f| %>
    <%= f.input :name %>
    <%= f.submit "Add a cocktail", class: "btn btn-outline-secondary" %>
  <% end %>
  <br>
    <a href="/cocktails" class="btn btn-outline-secondary">Back to cocktails</a>
</div>
